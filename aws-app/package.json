{
  "name": "aws-app",
  "version": "0.1.0",
  "bin": {
    "aws-app": "bin/aws-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "build-deploy": "tsc && cdk synth && cdk deploy -- --profile=$profile --require-approval=never",
    "buildAndDeploy": "cd ../react-app && npm run buildAndCopy && cd ../aws-app && npm run build-deploy --profile=$profile"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "2.105.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.105.0",
    "constructs": "^10.0.0"
  }
}
